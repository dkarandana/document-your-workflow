{
	"plugins": ["node_modules/jsdoc/plugins/markdown"],
	"recurseDepth": 10,
	"source": {
		"include": ["src/js/", "src/js/modules/core"],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	}
}